  <script type="text/ng-template" id="eventcombo.tmpl.html">
    <md-dialog aria-label="Contact Eventcombo">
      <form name="eventcomboForm" novalidate>
        <md-dialog-content>
          <div class="md-dialog-content">
            <h2 class="cntTitle">Need to get in touch? We're listening.</h2>
            <p class="cntSubtitle mb-4 pb-2">There's a good chance we have an answer in our <a href="/support">FAQ & Support.</a> <br />Didn't find what you're looking for? Complete the form below and we'll get back to you right away.</p>
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>Name</label>
              <input md-no-asterisk name="cntName" ng-model="cntName" required ng-keypress="submitted=false">
              <div ng-messages="eventcomboForm.cntName.$error">
                <div ng-message="required">Please enter your Name!</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block max350">
              <label>Email</label>
              <input type="email" required name="cntEmail" ng-model="cntEmail" ng-pattern="/^.+@.+\..+$/" ng-keypress="submitted=false" />
              <div ng-messages="eventcomboForm.cntEmail.$error" role="alert">
                <div ng-message="required">Please enter your Email Address!</div>
                <div ng-message="pattern">Please use valid email format!</div>
              </div>
            </md-input-container>
            <ec-phone-field class="md-block max350" form="eventcomboForm" name="cntPhone" phone-number-model="cntPhone" label="'Phone Number'" ng-required="true"></ec-phone-field>
            <md-input-container class="width160">
              <label>Category</label>
              <md-select name="cntCategory" ng-model="SelectedCategory.CategoryId" ng-change="updateCategory()" required>
                <md-option ng-repeat="category in Categories" value="{{category.Id}}">
                  {{category.Title}}
                </md-option>
              </md-select>
              <div ng-messages="eventcomboForm.cntCategory.$error">
                <div ng-message="required">Please select category!</div>
              </div>
            </md-input-container>
            <md-input-container class="width160">
              <label>Sub Category</label>
              <md-select ng-model="SelectedCategory.SubCategoryId" name="cntSubCategory" ng-change="updateSubCategory()" required>
                <md-option ng-repeat="subcategory in SubCategories" value="{{subcategory.Id}}">
                  {{subcategory.Title}}
                </md-option>
              </md-select>
              <div ng-messages="eventcomboForm.cntSubCategory.$error">
                <div ng-message="required">Please select sub category!</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block">
              <label>Message</label>
              <textarea name="cntMessage" ng-model="cntMessage" rows="3" md-select-on-focus md-maxlength="500" required></textarea>
              <div ng-messages="eventcomboForm.cntMessage.$error">
                <div ng-message="required">Please enter message!</div>
                <div ng-message="md-maxlength">Message is too long!</div>
              </div>
            </md-input-container>
            <div vc-recaptcha
                 theme="'light'"
                 key="'6LcyOz8UAAAAAPXj5hx6GCkHiLAqwH1rNC64xtZD'"
                 on-create="setWidgetId(widgetId)"
                 on-success="setResponse(response)"
                 on-expire="cbExpiration()"
                 data-size="compact">
            </div>
          </div>
        </md-dialog-content>
          
        <md-dialog-actions layout="row">
            <md-button class="cntButton" ng-click="cancel()">Cancel</md-button>
            <md-button class="cntButton" type="submit" ng-click="sendContactMessageEventcombo('eventcomboForm')">Submit</md-button>
        </md-dialog-actions>
      </form>
    </md-dialog>
  </script>
  <script type="text/ng-template" id="forwardfriend.tmpl.html">
    <md-dialog aria-label="Forward to Friend">
      <form name="forwardfriendForm" novalidate>
        <md-dialog-content>
          <div class="md-dialog-content">
            <h2 class="cntTitle" ng-bind-html="CMessage.Title+' - Forward to Friend(s)'"></h2>
            <input type="hidden" name="Id" ng-model="Id" value="{{CMessage.Id}}" />
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>My Name</label>
              <input md-no-asterisk name="myName" ng-model="CMessage.Name" required ng-keypress="submitted=false">
              <div ng-messages="forwardfriendForm.myName.$error">
                  <div ng-message="required">Please enter your name!</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>My Email Address</label>
              <input type="email" required name="myEmail" ng-model="CMessage.Email" ng-pattern="/^.+@.+\..+$/" ng-keypress="submitted=false" />
              <div ng-messages="forwardfriendForm.myEmail.$error" role="alert">
                  <div ng-message="required">Please enter your email address!</div>
                <div ng-message="pattern">Please use valid email format!</div>
              </div>
            </md-input-container>
            <md-input-container layout="column">
              <label>Send to</label>
              <md-chips name="sendTo" class="sendTo" ng-model="CMessage.To" ng-init="keys=[13,188]" md-separator-keys="keys" placeholder="Enter an email address" type="email" md-transform-chip="chipValidation($chip)" md-require-match="true" md-add-on-blur="true"></md-chips>
              <ng-messages class="showError" ng-show="isEmailValid">Please use valid email format!</ng-messages>
              <ng-messages class="showError" ng-show="CMessage.IsEmailRequired">Please enter send to email!</ng-messages>
              <div ng-messages="forwardfriendForm.sendTo.$error" role="alert">
                <div ng-message="pattern">Please use valid email format!</div>
                <div ng-message="required">Please enter your email address!</div>
              </div>
              <div class="hint showHint">Enter multiple email addresses by using a comma after each email (press enter on mobile)</div>
            </md-input-container>
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>Subject</label>
              <input name="Subject" ng-model="CMessage.Subject" required ng-bind-html="CMessage.Title" />
              <div ng-messages="forwardfriendForm.Subject.$error">
                <div ng-message="required">Please enter subject!</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block">
              <label>Message</label>
              <textarea name="cntMessage" ng-model="CMessage.Message" required rows="3" md-select-on-focus></textarea>
              <div ng-messages="forwardfriendForm.cntMessage.$error">
                  <div ng-message="required">Please enter your message!</div>
              </div>
            </md-input-container>
            <div vc-recaptcha
                 theme="'light'"
                 key="'6LcyOz8UAAAAAPXj5hx6GCkHiLAqwH1rNC64xtZD'"
                 on-create="setWidgetId(widgetId)"
                 on-success="setResponse(response)"
                 on-expire="cbExpiration()"
                 data-size="compact">
            </div>
            <md-dialog-actions layout="row">
              <md-button class="cntButton" ng-click="cancel()">Cancel</md-button>
              <md-button class="cntButton" type="submit" ng-click="sendFriendMessage('forwardfriendForm')">Send</md-button>
            </md-dialog-actions>
          </div>
          <div id="reCaptchaForwardFriend"></div>
        </md-dialog-content>
      </form>
    </md-dialog>
  </script>
  <script type="text/ng-template" id="organizer.tmpl.html">
    <md-dialog aria-label="Contact Organizer">
      <form name="organizerForm" novalidate>
        <md-dialog-content>
          <div class="md-dialog-content">
            <h2 class="cntTitle" ng-bind-html="'Contact '+CMessage.OrganizerName+' for '+CMessage.EventName"></h2>
            <p class="cntSubtitle">Contact {{CMessage.OrganizerName}} if you have any questions or comments regarding their event</p>
            <input type="hidden" name="EventId" ng-model="CMessage.EventId" value="{{CMessage.EventId}}" />
            <input type="hidden" name="OrganizerId" ng-model="CMessage.OrganizerId" value="{{CMessage.OrganizerId}}" />
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>Name</label>
              <input md-no-asterisk name="cntName" ng-model="CMessage.Name" required ng-keypress="submitted=false">
              <div ng-messages="organizerForm.cntName.$error">
                  <div ng-message="required">Please enter your name!</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>Email</label>
              <input type="email" required name="cntEmail" ng-model="CMessage.Email" ng-pattern="/^.+@.+\..+$/" ng-keypress="submitted=false" />
              <div ng-messages="organizerForm.cntEmail.$error" role="alert">
                  <div ng-message="required">Please enter your email address!</div>
                  <div ng-message="pattern">Please use valid email format!</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block margin-2em-bottom max350">
              <label>Phone</label>
              <input tel-mask name="cntPhone" ng-model="CMessage.Phone" required />
              <div ng-messages="organizerForm.cntPhone.$error">
                  <div ng-message="required">Please enter your phone number!</div>
                <div ng-message="phonePattern">Please enter a valid phone number pattern - ###-###-####</div>
              </div>
            </md-input-container>
            <md-input-container class="md-block">
              <label>Message</label>
              <textarea name="cntMessage" ng-model="CMessage.Message" required rows="3" md-select-on-focus></textarea>
              <div ng-messages="organizerForm.cntMessage.$error">
                  <div ng-message="required">Please enter your message!</div>
              </div>
            </md-input-container>
            <div vc-recaptcha
                 theme="'light'"
                 key="'6LcyOz8UAAAAAPXj5hx6GCkHiLAqwH1rNC64xtZD'"
                 on-create="setWidgetId(widgetId)"
                 on-success="setResponse(response)"
                 on-expire="cbExpiration()"
                 data-size="compact">
            </div>
            <md-dialog-actions layout="row">
              <md-button class="cntButton" ng-click="cancel()">Cancel</md-button>
              <md-button class="cntButton" type="submit" ng-click="sendOrganizerMessage('organizerForm')">Send</md-button>
            </md-dialog-actions>
          </div>
        </md-dialog-content>
      </form>
    </md-dialog>
  </script>
  <script type="text/ng-template" id="eventsubmit.tmpl.html">
    <md-dialog aria-label="Event Submit Confirmation" style="width:initial;">
      <md-dialog-content>
        <div class="md-dialog-content">
          <h2 class="cntTitle">You did great! Your event is now live.</h2>
          <div class="sortLink" title="{{EventParameters.EventUrl}}" id="copyTarget">{{EventParameters.EventUrl}}</div>
          <div class="scContainer">
            <div class="scButtonBox">
              <a class="scButton pink" onclick="copyToClipboard('copyTarget')">Copy to clipboard</a>
              <a class="scButton darkBlue" ng-click="shareEvent()">Share on facebook</a>
              <a class="scButton green" ng-href="{{EventParameters.EventUrl}}">View your event</a>
              <a class="scButton lightBlue" ng-click="editEvent()">Edit your event</a>
            </div>
            <div class="scButtonBox">
              <img ng-src="{{EventParameters.QrCodePath}}" />
            </div>
          </div>
        </div>
      </md-dialog-content>
      <md-dialog-actions layout="row">
        <md-button class="cntButton" ng-click="showDashboard()">Close</md-button>
      </md-dialog-actions>
    </md-dialog>
  </script>
    <script type="text/ng-template" id="friendtofriend.tmpl.html">
