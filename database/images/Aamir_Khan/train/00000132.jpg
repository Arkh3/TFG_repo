    <script type="text/javascript">
      function getFormatedName(name) {
        var formatedName = '';
        if (name) {
          var arry = name.split('-');
          formatedName = arry[0] + ": " + arry[1];
        }
        return formatedName;
      }

      function CreateSeatInfo(info) {
        var seatInfo = '';
        if (!(info.Row == '')) {
          if (info.Row === 'GeneralSection') {
            seatInfo = getFormatedName(info.Section)
          } else {
            seatInfo = getFormatedName(info.Section) + ' ' + getFormatedName(info.Row) + ' ' + getFormatedName(info.Seat);
          }
        } else {
          seatInfo = getFormatedName(info.Section) + ' ' + getFormatedName(info.Seat);
        }

        return seatInfo;
      }

    </script>
