function getBrowserName() {
  var name = "";
  if ((/(chrome\/)(\d+\.\d+)/i.test(navigator.userAgent) && navigator.userAgent.search("UCBrowser") < 0) && (/(chrome\/)(\d+\.\d+)/i.test(navigator.userAgent) && navigator.userAgent.search("Edge") < 0) && (/(chrome\/)(\d+\.\d+)/i.test(navigator.userAgent) && navigator.userAgent.search("UBrowser") < 0)){
      name = "Chrome";
  }else if (navigator.userAgent.indexOf("MSIE") != -1) {
      name = "MSIE";
  } else if (navigator.userAgent.indexOf("Firefox") != -1) {
      name = "Firefox";
  } else if (navigator.userAgent.indexOf("Opera") != -1) {
      name = "Opera";
  } else if (navigator.userAgent.indexOf("Safari") != -1) {
      name = "Safari";
  }
  return name;
}
